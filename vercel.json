{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node",
            "config": {
                "installCommand": "npm install"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "functions": {
        "utils/pdfGenerator.js": {
            "maxDuration": 60
        }
    },
    "env": {
        "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true"
    },
    "regions": [
        "iad1"
    ]
}