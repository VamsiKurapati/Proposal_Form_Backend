{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node",
            "config": {
                "installCommand": "npm install"
            }
        }
    ],
    "functions": {
        "api/proposals/generatePDF": {
            "maxDuration": 120
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "regions": [
        "iad1"
    ],
    "env": {
        "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true",
        "PUPPETEER_EXECUTABLE_PATH": "/tmp/chromium",
        "NODE_ENV": "production"
    },
    "build": {
        "env": {
            "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true"
        }
    }
}